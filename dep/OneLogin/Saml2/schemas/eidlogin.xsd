	<complexType name="PlaceVerificationRequestType">
		<sequence>
			<element name="CommunityID">
				<simpleType>
					<restriction base="string">
						<pattern
							value="[0][0-9]{3}([0-9]{2}([0][0-9]([0-9]{2}([0][0-9]{3})?)?)?)?" />
					</restriction>
				</simpleType>
			</element>
		</sequence>
	</complexType>
