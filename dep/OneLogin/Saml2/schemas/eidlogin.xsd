	<simpleType name="LevelOfAssuranceType">
		<restriction base="string">
			<enumeration value="http://eidas.europa.eu/LoA/low" />
			<enumeration value="http://eidas.europa.eu/LoA/substantial" />
			<enumeration value="http://eidas.europa.eu/LoA/high" />
			<enumeration value="http://bsi.bund.de/eID/LoA/normal"></enumeration>
			<enumeration
				value="http://bsi.bund.de/eID/LoA/substantiell">
			</enumeration>
			<enumeration value="http://bsi.bund.de/eID/LoA/hoch"></enumeration>
		</restriction>
	</simpleType>

