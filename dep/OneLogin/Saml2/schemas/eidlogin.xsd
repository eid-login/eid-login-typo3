	targetNamespace="http://bsi.bund.de/eID/"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	version="1.0.0 2021-02-23">

	<import namespace="urn:oasis:names:tc:SAML:2.0:assertion"
		schemaLocation="saml-schema-assertion-2.0.xsd" />
	<import namespace="urn:oasis:names:tc:SAML:2.0:protocol"
		schemaLocation="saml-schema-protocol-2.0.xsd" />
	<import namespace="http://www.skidentity.de/att"
		schemaLocation="skidentity-attributes.xsd" />

